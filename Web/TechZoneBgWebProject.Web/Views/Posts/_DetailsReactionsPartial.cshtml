@model TechZoneBgWebProject.Web.ViewModels.Posts.PostsDetailsViewModel

<button type="button" class="tt-icon-btn" style="padding: 0; border:none; background:none; outline: none; color: #666f74" onclick="likeReaction(@Model.Id, 'post-reactions', 'post')">
    <i class="tt-icon"><img src="~/images/like.svg" alt="like" /></i>
</button>
<button type="button" class="tt-icon-btn" style="padding: 0; border:none; background:none; outline: none; color: #666f74" onclick="dislikeReaction(@Model.Id, 'post-reactions', 'post')">
    <i class="tt-icon"><img src="~/images/dislike.svg" alt="dislike" /></i>
</button>

