@{ ViewData["Title"] = "ERROR 404"; }

<div class="container">
    <div class="tt-layout-404">
        <span class="tt-icon">
            <svg class="icon">
                <use xlink:href="#icon-error_404"></use>
            </svg>
        </span>
        <h6 class="tt-title">@ViewData["Title"]</h6>
        <p>
            Sorry we can’t find what you are looking for, here’s some<br />
            <a asp-area=""
               asp-controller="Posts"
               asp-action="Trending"
               class="tt-color-dark tt-underline-02">
                suggested posts
            </a> for you.
        </p>
    </div>
</div>
